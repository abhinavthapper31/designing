    {
      "id": "dV9-uwDVcmtWZtAyld-lq",
      "type": "text",
      "x": -133.13030300342248,
      "y": 3047.0051092368,
      "width": 899.793212890625,
      "height": 104.91992402941102,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
      "strokeWidth": 2,
      "strokeStyle": "solid",
